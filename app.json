{
  "expo": {
    "name": "Homeis - מנהל ההוצאות שלך",
    "slug": "home-expense-manager",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/images/main_logo_icon.png",
    "scheme": "home-expense-manager",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "אפליקציה חכמה לניהול הוצאות משותפות בין חברים או שותפים. מאפשרת מעקב אחר הוצאות, ניהול תקציב משותף, חלוקת הוצאות בין דיירים, וניתוח מגמות צריכה. כוללת תמיכה בקטגוריות מותאמות אישית, העלאת קבלות, והתראות חכמות.",
    "ios": {
      "supportsTablet": true,
      "buildNumber": "35",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "אפליקציה זו זקוקה לגישה לגלריה כדי להוסיף תמונות להוצאות",
        "NSCameraUsageDescription": "אפליקציה זו זקוקה לגישה למצלמה כדי לצלם תמונות להוצאות",
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"]
      },
      "bundleIdentifier": "com.sarusiziv96.homeexpensemanager"
    },
    "android": {
      "versionCode": 2,
      "icon": "./assets/images/main_logo_icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/main_logo_icon.png",
        "backgroundColor": "#3DDC84"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.CAMERA",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK"
      ],
      "package": "com.sarusiziv96.homeexpensemanager"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/main_logo_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-4216673023528064~5049656788",
          "iosAppId": "ca-app-pub-4216673023528064~5049656788"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/main_logo_icon.png",
          "color": "#3DDC84",
          "defaultChannel": "default"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9fc51341-7952-470b-8341-39d792d970b5"
      }
    }
  }
}